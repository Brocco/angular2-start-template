{
  "name": "angular2-start-template",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -p src -w",
    "clean": "rimraf ./dist/",
    "prebuild": "npm-run-all clean",
    "build": "webpack -d",
    "postbuild": "cp ./src/index.html ./dist/",
    "prestart": "npm-run-all -s clean build",
    "build:watch": "webpack -d -w",
    "live-server": "live-server ./dist",
    "start": "npm-run-all -s build -p build:watch live-server"
  },
  "author": "Mike Brocchi",
  "license": "MIT",
  "homepage": "https://github.com/Brocco/angular2-start-template#readme",
  "dependencies": {
    "@reactivex/rxjs": "^5.0.0-alpha.10",
    "angular2": "2.0.0-alpha.46",
    "reflect-metadata": "^0.1.2",
    "systemjs": "0.19.6",
    "zone.js": "^0.5.8"
  },
  "devDependencies": {
    "live-server": "^0.8.1",
    "npm-run-all": "^1.3.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.1",
    "typescript": "^1.6.2",
    "webpack": "^1.12.8",
    "webpack-dev-server": "^1.12.1"
  },
  "keywords": [],
  "description": ""
}
